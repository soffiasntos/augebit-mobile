{
  "expo": {
    "name": "augebit-mobile",
    "slug": "augebit-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/augebit.png",
    "splash": {
      "image": "./assets/augebit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/augebit.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.augebitmobile"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
